{
  "expo": {
    "name": "PoketDrive",
    "slug": "poketdrive",
    "scheme": "poketdrive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/poketdrive.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6633"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.crakton.poketdrive",
      "userInterfaceStyle": "automatic",
      "config": {
        "googleMapsApiKey": "GOOGLE_MAPS_APIKEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/poketdrive.png",
        "backgroundColor": "#FF6633"
      },
      "package": "com.crakton.poketdrive",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "poketdrive"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_MAPS_APIKEY"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/poketdrive.png"
    },
    "extra": {
      "eas": {
        "projectId": "2098109f-b648-4494-a495-f9da33d261c1"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ]
  }
}