{
  "expo": {
    "name": "ur-ride",
    "slug": "ur-ride",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SplashNew.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SplashNew.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6633"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omenvc.urride",
      "userInterfaceStyle": "automatic",
      "config": {
        "googleMapsApiKey": "GOOGLE_MAPS_APIKEY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SplashNew.png",
        "backgroundColor": "#FF6633"
      },
      "package": "com.omenvc.urride",
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_MAPS_APIKEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/SplashNew.png"
    },
    "extra": {
      "eas": {
        "projectId": "0c7cd1c9-d32d-4672-b431-89cb793f3ec7"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show current location on map."
        }
      ]
    ]
  }
}
